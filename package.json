  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/jest": "^24.0.11",
    "babel-eslint": "^10.0.1",
    "babel-watch": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
